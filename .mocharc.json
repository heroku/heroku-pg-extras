{
  "extension": ["ts"],
  "spec": "test/**/*.test.ts",
  "require": ["ts-node/register", "test/helpers.mjs"],
  "package": "./package.json",
  "reporter": "spec",
  "timeout": 5000,
  "recursive": true,
  "watchExtension": "ts",
  "node-option": [
    "loader=ts-node/esm",
    "experimental-specifier-resolution=node"
  ]
}
