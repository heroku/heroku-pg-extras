{
  "name": "@heroku-cli/heroku-pg-extras",
  "description": "Heroku PG Extras CLI plugin",
  "version": "2.0.0",
  "author": "Heroku",
  "bugs": {
    "url": "https://github.com/heroku/heroku-pg-extras/issues"
  },
  "dependencies": {
    "@heroku-cli/command": "^11.5.0",
    "@heroku-cli/schema": "^2.0.0",
    "@heroku/heroku-cli-util": "9.1.3",
    "@oclif/core": "^2.16.0",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^2.0.0",
    "@types/chai": "^4.3.13",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^4.0.0",
    "c8": "^7.7.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mocha": "^10.4.3",
    "mocha": "^10.8.2",
    "mock-stdin": "^1.0.0",
    "nock": "^13.2.9",
    "oclif": "^4.22.5",
    "sinon": "^18.0.1",
    "sinon-chai": "^3.7.0",
    "stdout-stderr": "^0.1.13",
    "strip-ansi": "^6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsheredoc": "^1.0.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "/dist",
    "oclif.manifest.json",
    "README.md"
  ],
  "homepage": "https://github.com/heroku/heroku-pg-extras",
  "keywords": [
    "heroku-plugin",
    "oclif"
  ],
  "license": "ISC",
  "oclif": {
    "bin": "heroku",
    "commands": "./dist/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "pg": {
        "description": "manage Heroku PostgreSQL databases"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/heroku-pg-extras.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b && oclif manifest && oclif readme",
    "build:dev": "rm -rf dist && tsc -b --sourcemap && oclif manifest && oclif readme",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts --config .eslintrc.json --fix",
    "posttest": "npm run lint",
    "test": "c8 mocha --forbid-only",
    "test:local": "c8 mocha",
    "version": "oclif readme && git add README.md",
    "prepare": "npm run build"
  }
}
